<Window x:Class="GenealogicalTreeCource.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GenealogicalTreeCource"
        mc:Ignorable="d" ResizeMode="NoResize"
        Title="MainWindow" Height="640" Width="1000">
    <Grid>
        <Button x:Name="ggg" Content="ПОДВИТИСЬ" FontSize="30" HorizontalAlignment="Left" Margin="202,364,0,0" VerticalAlignment="Top" Height="122" Width="243" Click="TreeButton_Click"/>
        <Button Content="ДОДАТИ"  FontSize="30" HorizontalAlignment="Left" Margin="560,365,0,0" VerticalAlignment="Top" Height="122" Width="243" Click="AddButton_Click"/>
        <Button Content="АДМІНІСТРАТОР" HorizontalAlignment="Left" Margin="888,23,0,0" VerticalAlignment="Top" Height="65" Width="80" Click="AdministratorButton_Click"/>
        <Border Margin="10">
            <Frame x:Name="SetWindow" NavigationUIVisibility="Hidden"/>
        </Border>
        <Button Content="Назад" Margin="893,566,16,16" Click="BackButton_Click" Visibility="{Binding ElementName=SetWindow}"/>
    </Grid>
</Window>
